{
    "name": "jonasarts/registry2-bundle",
    "description": "A Registry System for Symfony",
    "keywords": ["registry", "key", "value", "store"],
    "homepage": "https://github.com/jonasarts/registry2-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Hauser",
            "email": "symfony@jonasarts.com",
            "homepage": "http://www.jonasarts.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "friendsofphp/proxy-manager-lts": "*",
        "symfony/config": "^5.0|^6.0",
        "symfony/dependency-injection": "^5.0|^6.0",
        "snc/redis-bundle": "^4.6"
    },
    "suggest": {
        "doctrine/annotations": "If you want to use DoctrineRegistry",
        "doctrine/doctrine-bundle": "If you want to use the DoctrineRegistry.",
        "snc/redis-bundle": "If you want to use RedisRegistry.",
        "predis/predis": "If you want to use predis for RedisRegistry."
    },
    "autoload": {
        "psr-4": { "jonasarts\\Bundle\\RegistryBundle\\": "src/" }
    }
}
