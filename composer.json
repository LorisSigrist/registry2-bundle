    "keywords": ["registry", "key", "value", "store"],
    "homepage": "https://github.com/jonasarts/registry2-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Hauser",
            "email": "symfony@jonasarts.com",
            "homepage": "http://www.jonasarts.com"
        }
    ],
    "require": {
        "doctrine/orm": "^2.5",
        "php": "^7.1",
        "predis/predis": "^1.0",
        "symfony/dependency-injection": "^3.0|^4.0",
        "symfony/validator": "^3.0|^4.0",
        "snc/redis-bundle": "^2.0"
    },
    "require-dev": {
        "symfony/form": "^4.0",
        "twig/twig": "^2.4.4"
    },
    "suggest": {
        "sensio/framework-extra-bundle": "If you want to use the builtin controller.",
